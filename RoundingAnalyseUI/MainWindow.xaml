<Window x:Class="RoundingAnalyseUI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="RoundingAnalyse" Height="600" Width="800">
    <Grid>
        <Menu HorizontalAlignment="Left" Height="100" Margin="14,5,0,0" VerticalAlignment="Top" Width="100" RenderTransformOrigin="0.5,0.5">
            <Menu.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="-0.693"/>
                    <TranslateTransform/>
                </TransformGroup>
            </Menu.RenderTransform>
        </Menu>

    </Grid>
</Window>
